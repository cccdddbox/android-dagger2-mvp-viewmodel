<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:id="@+id/my_firstpage"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent"
                                             tools:context="com.harry.dagger2mvpviewmodelhapp.ui.first.FirstActivity">

    <TextView
            android:id="@+id/my_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="48sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    <Button
            android:id="@+id/my_firstbtn"
            android:text="@string/first_nav"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
             app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="148dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="148dp"
            android:layout_marginBottom="40dp" app:layout_constraintBottom_toBottomOf="parent"/>


</android.support.constraint.ConstraintLayout>
